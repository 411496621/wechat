/* page/index/index.wxss */
page{
  width: 100%;
  height: 100%;
}

.indexContainer{
  height: 100%;
  background:#8ED145; 
  display: flex;
  flex-direction:column;
  align-items:center;
}
image{
  width: 200rpx;
  height: 200rpx;
  border-radius:100rpx;
  margin: 100rpx 0;
}
.userInfo{
  margin: 100rpx 0;
  font-size: 40rpx;
  font-weight: 700;
}

.start{
  width: 300rpx;
  height:80rpx;
  text-align: center;
  line-height:80rpx;
  font-size:28rpx;
  border-radius: 8rpx;
  border:1rpx solid #666;
}